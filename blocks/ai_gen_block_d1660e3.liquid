{% doc %}
  @prompt
    add a mood based shopping corousel, has different mood of shopping, enable the option to add the background and make the mood section modern card like style, in mood shopping corousel section only, give option of images in section background
{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-mood-carousel-{{ ai_gen_id }} {
    width: 100%;
    max-width: 100%;
    padding: {{ block.settings.section_padding }}px 20px;
    display: block;
    position: relative;
    {% if block.settings.section_background_image %}
      background-image: url('{{ block.settings.section_background_image | image_url: width: 2000 }}');
      background-size: cover;
      background-position: center;
      background-repeat: no-repeat;
    {% else %}
      background-color: {{ block.settings.section_background }};
    {% endif %}
  }

  .ai-mood-carousel__background-overlay-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: {{ block.settings.section_background_overlay }};
    opacity: {{ block.settings.section_background_overlay_opacity | divided_by: 100.0 }};
    pointer-events: none;
  }

  .ai-mood-carousel__wrapper-{{ ai_gen_id }} {
    max-width: {{ block.settings.desktop_width_percent }}%;
    margin: 0 auto;
    position: relative;
    z-index: 1;
  }

  .ai-mood-carousel__header-{{ ai_gen_id }} {
    text-align: center;
    margin-bottom: 40px;
  }

  .ai-mood-carousel__title-{{ ai_gen_id }} {
    font-size: {{ block.settings.title_size }}px;
    color: {{ block.settings.title_color }};
    margin: 0 0 12px;
    text-transform: {{ block.settings.title_case }};
  }

  .ai-mood-carousel__subtitle-{{ ai_gen_id }} {
    font-size: {{ block.settings.subtitle_size }}px;
    color: {{ block.settings.subtitle_color }};
    margin: 0;
  }

  .ai-mood-carousel__container-{{ ai_gen_id }} {
    position: relative;
    overflow: hidden;
    padding: 0 50px;
  }

  .ai-mood-carousel__track-{{ ai_gen_id }} {
    display: flex;
    gap: {{ block.settings.card_gap }}px;
    transition: transform 0.5s ease;
  }

  .ai-mood-carousel__card-{{ ai_gen_id }} {
    flex: 0 0 calc((100% - {{ block.settings.card_gap | times: 2 }}px) / 3);
    background: {{ block.settings.card_background }};
    border-radius: {{ block.settings.card_border_radius }}px;
    overflow: hidden;
    cursor: pointer;
    transition: all 0.4s cubic-bezier(0.4, 0, 0.2, 1);
    text-decoration: none;
    display: block;
    box-shadow: {{ block.settings.card_shadow_x }}px {{ block.settings.card_shadow_y }}px {{ block.settings.card_shadow_blur }}px rgba(0, 0, 0, {{ block.settings.card_shadow_opacity | divided_by: 100.0 }});
    border: {{ block.settings.card_border_width }}px solid {{ block.settings.card_border_color }};
  }

  .ai-mood-carousel__card-{{ ai_gen_id }}:hover {
    transform: translateY(-{{ block.settings.card_hover_lift }}px) scale({{ block.settings.card_hover_scale | divided_by: 100.0 }});
    box-shadow: {{ block.settings.card_hover_shadow_x }}px {{ block.settings.card_hover_shadow_y }}px {{ block.settings.card_hover_shadow_blur }}px rgba(0, 0, 0, {{ block.settings.card_hover_shadow_opacity | divided_by: 100.0 }});
  }

  .ai-mood-carousel__card-image-wrapper-{{ ai_gen_id }} {
    position: relative;
    width: 100%;
    padding-bottom: {{ block.settings.card_image_aspect_ratio }}%;
    overflow: hidden;
    background-color: #f4f4f4;
  }

  .ai-mood-carousel__card-image-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    object-fit: cover;
    transition: transform 0.4s ease;
  }

  .ai-mood-carousel__card-{{ ai_gen_id }}:hover .ai-mood-carousel__card-image-{{ ai_gen_id }} {
    transform: scale({{ block.settings.image_hover_scale | divided_by: 100.0 }});
  }

  .ai-mood-carousel__card-image-wrapper-{{ ai_gen_id }} svg {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
  }

  .ai-mood-carousel__card-overlay-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: linear-gradient(to bottom, rgba(0,0,0,0) 0%, rgba(0,0,0,{{ block.settings.overlay_opacity | divided_by: 100.0 }}) 100%);
    display: flex;
    align-items: flex-end;
    padding: {{ block.settings.card_content_padding }}px;
  }

  .ai-mood-carousel__card-mood-{{ ai_gen_id }} {
    color: {{ block.settings.mood_text_color }};
    font-size: {{ block.settings.mood_text_size }}px;
    font-weight: 600;
    margin: 0;
    text-transform: {{ block.settings.mood_text_case }};
    text-shadow: 0 2px 4px rgba(0, 0, 0, 0.3);
  }

  .ai-mood-carousel__card-content-{{ ai_gen_id }} {
    padding: {{ block.settings.card_content_padding }}px;
  }

  .ai-mood-carousel__card-description-{{ ai_gen_id }} {
    color: {{ block.settings.description_color }};
    font-size: {{ block.settings.description_size }}px;
    margin: 0 0 12px;
    line-height: 1.5;
  }

  .ai-mood-carousel__card-cta-{{ ai_gen_id }} {
    color: {{ block.settings.cta_color }};
    font-size: 14px;
    font-weight: 600;
    text-decoration: none;
    display: inline-flex;
    align-items: center;
    gap: 6px;
    transition: gap 0.3s ease;
  }

  .ai-mood-carousel__card-{{ ai_gen_id }}:hover .ai-mood-carousel__card-cta-{{ ai_gen_id }} {
    color: {{ block.settings.cta_hover_color }};
    gap: 10px;
  }

  .ai-mood-carousel__nav-{{ ai_gen_id }} {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    background-color: {{ block.settings.nav_button_background }};
    color: {{ block.settings.nav_button_color }};
    border: none;
    width: 44px;
    height: 44px;
    border-radius: 50%;
    cursor: pointer;
    display: flex;
    align-items: center;
    justify-content: center;
    transition: all 0.3s ease;
    z-index: 2;
    box-shadow: 0 2px 8px rgba(0, 0, 0, 0.15);
  }

  .ai-mood-carousel__nav-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.nav_button_hover_background }};
    transform: translateY(-50%) scale(1.1);
  }

  .ai-mood-carousel__nav-{{ ai_gen_id }}:disabled {
    opacity: 0.3;
    cursor: not-allowed;
  }

  .ai-mood-carousel__nav-{{ ai_gen_id }}:disabled:hover {
    transform: translateY(-50%) scale(1);
  }

  .ai-mood-carousel__nav--prev-{{ ai_gen_id }} {
    left: 0;
  }

  .ai-mood-carousel__nav--next-{{ ai_gen_id }} {
    right: 0;
  }

  .ai-mood-carousel__dots-{{ ai_gen_id }} {
    display: flex;
    justify-content: center;
    gap: 8px;
    margin-top: 30px;
  }

  .ai-mood-carousel__dot-{{ ai_gen_id }} {
    width: 10px;
    height: 10px;
    border-radius: 50%;
    background-color: {{ block.settings.dot_color }};
    border: none;
    cursor: pointer;
    padding: 0;
    transition: all 0.3s ease;
  }

  .ai-mood-carousel__dot-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.dot_active_color }};
    opacity: 0.7;
  }

  .ai-mood-carousel__dot-{{ ai_gen_id }}.active {
    background-color: {{ block.settings.dot_active_color }};
    transform: scale(1.3);
  }

  @media screen and (max-width: 989px) {
    .ai-mood-carousel__card-{{ ai_gen_id }} {
      flex: 0 0 calc((100% - {{ block.settings.card_gap }}px) / 2);
    }
  }

  @media screen and (max-width: 749px) {
    .ai-mood-carousel__container-{{ ai_gen_id }} {
      padding: 0 40px;
    }

    .ai-mood-carousel__card-{{ ai_gen_id }} {
      flex: 0 0 100%;
    }

    .ai-mood-carousel__title-{{ ai_gen_id }} {
      font-size: {{ block.settings.title_size | times: 0.7 }}px;
    }

    .ai-mood-carousel__subtitle-{{ ai_gen_id }} {
      font-size: {{ block.settings.subtitle_size | times: 0.9 }}px;
    }

    .ai-mood-carousel__nav-{{ ai_gen_id }} {
      width: 36px;
      height: 36px;
    }
  }
{% endstyle %}

<mood-carousel-{{ ai_gen_id }}
  class="ai-mood-carousel-{{ ai_gen_id }}"
  {{ block.shopify_attributes }}
>
  <div class="ai-mood-carousel__background-overlay-{{ ai_gen_id }}"></div>
  <div class="ai-mood-carousel__wrapper-{{ ai_gen_id }}">
    <div class="ai-mood-carousel__header-{{ ai_gen_id }}">
      {% if block.settings.title != blank %}
        <h2 class="ai-mood-carousel__title-{{ ai_gen_id }}">{{ block.settings.title }}</h2>
      {% endif %}
      {% if block.settings.subtitle != blank %}
        <p class="ai-mood-carousel__subtitle-{{ ai_gen_id }}">{{ block.settings.subtitle }}</p>
      {% endif %}
    </div>

    <div class="ai-mood-carousel__container-{{ ai_gen_id }}">
      <button
        class="ai-mood-carousel__nav-{{ ai_gen_id }} ai-mood-carousel__nav--prev-{{ ai_gen_id }}"
        aria-label="Previous"
        data-nav="prev"
      >
        <svg width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
          <polyline points="15 18 9 12 15 6"></polyline>
        </svg>
      </button>

      <div class="ai-mood-carousel__track-{{ ai_gen_id }}" data-track>
        {% for i in (1..6) %}
          {% liquid
            assign mood_enabled_key = 'mood_' | append: i | append: '_enabled'
            assign mood_name_key = 'mood_' | append: i | append: '_name'
            assign mood_description_key = 'mood_' | append: i | append: '_description'
            assign mood_image_key = 'mood_' | append: i | append: '_image'
            assign mood_collection_key = 'mood_' | append: i | append: '_collection'

            assign mood_enabled = block.settings[mood_enabled_key]
            assign mood_name = block.settings[mood_name_key]
            assign mood_description = block.settings[mood_description_key]
            assign mood_image = block.settings[mood_image_key]
            assign mood_collection = block.settings[mood_collection_key]
          %}

          {% if mood_enabled %}
            {% if mood_collection != blank %}
              <a
                href="{{ mood_collection.url }}"
                class="ai-mood-carousel__card-{{ ai_gen_id }}"
              >
            {% else %}
              <div class="ai-mood-carousel__card-{{ ai_gen_id }}">
            {% endif %}
              <div class="ai-mood-carousel__card-image-wrapper-{{ ai_gen_id }}">
                {% if mood_image %}
                  <img
                    src="{{ mood_image | image_url: width: 600 }}"
                    alt="{{ mood_name | escape }}"
                    class="ai-mood-carousel__card-image-{{ ai_gen_id }}"
                    loading="lazy"
                    width="600"
                    height="600"
                  >
                {% else %}
                  {{ 'collection-apparel-1' | placeholder_svg_tag }}
                {% endif %}
                <div class="ai-mood-carousel__card-overlay-{{ ai_gen_id }}">
                  <h3 class="ai-mood-carousel__card-mood-{{ ai_gen_id }}">{{ mood_name }}</h3>
                </div>
              </div>
              <div class="ai-mood-carousel__card-content-{{ ai_gen_id }}">
                {% if mood_description != blank %}
                  <p class="ai-mood-carousel__card-description-{{ ai_gen_id }}">{{ mood_description }}</p>
                {% endif %}
                {% if mood_collection != blank %}
                  <span class="ai-mood-carousel__card-cta-{{ ai_gen_id }}">
                    {{ block.settings.cta_text }}
                    <svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
                      <line x1="5" y1="12" x2="19" y2="12"></line>
                      <polyline points="12 5 19 12 12 19"></polyline>
                    </svg>
                  </span>
                {% endif %}
              </div>
            {% if mood_collection != blank %}
              </a>
            {% else %}
              </div>
            {% endif %}
          {% endif %}
        {% endfor %}
      </div>

      <button
        class="ai-mood-carousel__nav-{{ ai_gen_id }} ai-mood-carousel__nav--next-{{ ai_gen_id }}"
        aria-label="Next"
        data-nav="next"
      >
        <svg width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
          <polyline points="9 18 15 12 9 6"></polyline>
        </svg>
      </button>
    </div>

    <div class="ai-mood-carousel__dots-{{ ai_gen_id }}" data-dots></div>
  </div>
</mood-carousel-{{ ai_gen_id }}>

<script>
  (function() {
    class MoodCarousel{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
        this.currentIndex = 0;
        this.itemsPerView = 3;
      }

      connectedCallback() {
        this.track = this.querySelector('[data-track]');
        this.cards = Array.from(this.track.children);
        this.prevButton = this.querySelector('[data-nav="prev"]');
        this.nextButton = this.querySelector('[data-nav="next"]');
        this.dotsContainer = this.querySelector('[data-dots]');

        this.updateItemsPerView();
        this.createDots();
        this.setupEventListeners();
        this.updateCarousel();

        window.addEventListener('resize', () => {
          this.updateItemsPerView();
          this.createDots();
          this.updateCarousel();
        });
      }

      updateItemsPerView() {
        if (window.innerWidth <= 749) {
          this.itemsPerView = 1;
        } else if (window.innerWidth <= 989) {
          this.itemsPerView = 2;
        } else {
          this.itemsPerView = 3;
        }
      }

      createDots() {
        this.dotsContainer.innerHTML = '';
        const totalDots = Math.ceil(this.cards.length / this.itemsPerView);

        for (let i = 0; i < totalDots; i++) {
          const dot = document.createElement('button');
          dot.className = 'ai-mood-carousel__dot-{{ ai_gen_id }}';
          dot.setAttribute('aria-label', 'Go to slide ' + (i + 1));
          dot.addEventListener('click', () => this.goToSlide(i));
          this.dotsContainer.appendChild(dot);
        }

        this.dots = Array.from(this.dotsContainer.children);
      }

      setupEventListeners() {
        this.prevButton.addEventListener('click', () => this.prev());
        this.nextButton.addEventListener('click', () => this.next());
      }

      prev() {
        if (this.currentIndex > 0) {
          this.currentIndex--;
          this.updateCarousel();
        }
      }

      next() {
        const maxIndex = Math.ceil(this.cards.length / this.itemsPerView) - 1;
        if (this.currentIndex < maxIndex) {
          this.currentIndex++;
          this.updateCarousel();
        }
      }

      goToSlide(index) {
        this.currentIndex = index;
        this.updateCarousel();
      }

      updateCarousel() {
        const cardWidth = this.cards[0].offsetWidth;
        const gap = {{ block.settings.card_gap }};
        const offset = -(this.currentIndex * this.itemsPerView * (cardWidth + gap));

        this.track.style.transform = 'translateX(' + offset + 'px)';

        const maxIndex = Math.ceil(this.cards.length / this.itemsPerView) - 1;
        this.prevButton.disabled = this.currentIndex === 0;
        this.nextButton.disabled = this.currentIndex === maxIndex;

        this.dots.forEach((dot, index) => {
          if (index === this.currentIndex) {
            dot.classList.add('active');
          } else {
            dot.classList.remove('active');
          }
        });
      }
    }

    customElements.define('mood-carousel-{{ ai_gen_id }}', MoodCarousel{{ ai_gen_id }});
  })();
</script>

